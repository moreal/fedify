{
  "name": "@fedify/fixture",
  "version": "2.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fedify-dev/fedify/issues"
  },
  "funding": [
    "https://opencollective.com/fedify",
    "https://github.com/sponsors/dahlia"
  ],
  "type": "module",
  "main": "./dist/mod.cjs",
  "module": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/mod.d.ts",
        "require": "./dist/mod.d.cts",
        "default": "./dist/mod.d.ts"
      },
      "import": "./dist/mod.js",
      "require": "./dist/mod.cjs",
      "default": "./dist/mod.js"
    },
    "./fixtures/*": "./dist/fixtures/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "package.json"
  ],
  "dependencies": {
    "@fedify/vocab-runtime": "workspace:^",
    "@logtape/logtape": "catalog:",
    "@opentelemetry/core": "catalog:",
    "@opentelemetry/sdk-trace-base": "catalog:"
  },
  "devDependencies": {
    "@js-temporal/polyfill": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "urlpattern-polyfill": "catalog:"
  },
  "scripts": {
    "build:self": "tsdown",
    "build": "turbo run build:self --filter=@fedify/fixture...",
    "prepack": "pnpm build",
    "prepublish": "pnpm build",
    "pretest": "pnpm build",
    "test": "cd dist/ && node --test"
  },
  "private": true
}
